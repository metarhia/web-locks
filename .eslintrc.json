{
  "extends": ["metarhia", "plugin:prettier/recommended"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "Atomics": true,
    "SharedArrayBuffer": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "operator-linebreak": [
      "error",
      "after",
      { "overrides": { "?": "before", ":": "before" } }
    ]
  }
}
